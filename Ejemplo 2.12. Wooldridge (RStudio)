# en github
library(wooldridge) # cargar libreria de Wooldridge

# Otras liberias necesarias para la regresion lineal
library (tidyverse)
library (car)
library (boot)
library (QuantPsyc)
library (ggplot2)

# cargar datos
data("meap93")
force("meap93") # ver los datos
# Antes de empezar nuestro modelo
attach(meap93)
names(meap93)
class(meap93)
